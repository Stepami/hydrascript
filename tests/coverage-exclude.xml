<?xml version="1.0" encoding="utf-8"?>
<Configuration>
    <CodeCoverage>

        <Functions>
            <Exclude>
                <Function>.*ToString.*</Function>
                <Function>.*GetHashCode.*</Function>
                <Function>.*GetEnumerator.*</Function>
            </Exclude>
        </Functions>

        <Attributes>
            <Exclude>
                <Attribute>^System\.CodeDom\.Compiler\.GeneratedCodeAttribute$</Attribute>
            </Exclude>
        </Attributes>

    </CodeCoverage>
</Configuration>